{
  "schemaVersion": 1,
  "id": "feathermorph-client",
  "version": "${version}",
  "name": "FeatherMorph",
  "description": "Client mod for the FeatherMorph plugin",
  "authors": ["MATRIX-feather"],
  "contact":
  {
    "sources": "https://github.com/XiaMoZhiShi/MorphPluginClient",
    "homepage": "https://github.com/XiaMoZhiShi"
  },
  "license": "MIT",
  "icon": "assets/morphclient/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xiamomc.morph.client.MorphClient"
    ],
    "modmenu":[
      "xiamomc.morph.client.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "client.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/fabricmc/fabric/impl/client/rendering/WorldRenderContextImpl": ["xiamomc/morph/client/mixin/external/IDDD"]
    }
  },
  "depends": {
    "fabricloader": ">=0.12.9",
    "fabric": "*",
    "minecraft": ">1.19.4"
  }
}
